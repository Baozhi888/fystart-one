{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/types/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": "../fystart",
    "paths": {
      "@/*": ["./src/*"]
    },
    "strict": true,
    "types": [
      "vite-plugin-pwa/client",
    ]
  },
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
